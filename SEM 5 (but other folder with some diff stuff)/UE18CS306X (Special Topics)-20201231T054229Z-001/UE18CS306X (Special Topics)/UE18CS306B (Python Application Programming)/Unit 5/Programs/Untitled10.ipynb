    "import sqlite3\n",
    "conn=sqlite3.connect(\"PES.db\")\n",
    "conn.execute(\"UPDATE STUDENT SET NAME='PES' WHERE ID=123\")\n",
    "conn.commit()\n",
    "cursor=conn.execute(\"SELECT * FROM STUDENT\")\n",
    "for row in cursor:\n",
    "    print(\"ID=\",row[0])\n",
    "    print(\"NAME=\",row[1])\n",
    "    print(\"AGE=\",row[2])\n",
    "conn.close()"
   ]
