    "import socket\n",
    "HOSTS = [\n",
    "'google.com',\n",
    "'abc',\n",
    "'www.python.org',\n",
    "'pes.edu',\n",
    "]\n",
    "for host in HOSTS:\n",
    "    try:\n",
    "        print('{} : {}'.format(host, socket.gethostbyname(host)))\n",
    "    except socket.error as msg:\n",
    "        print('{} : {}'.format(host, msg))"
